-        ntoh_logif(&reply_from_fci);  /* set correct byte order */
-    }
-    
-    /* if search successful, then assign the data */
-    if (FPP_ERR_OK == rtn)
-    {
-        *p_rtn_logif = reply_from_fci;
-    }
-    
-    return (rtn);
-}
