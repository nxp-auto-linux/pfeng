-    assert(NULL != p_name);
-    assert(NULL != p_parent_name);
-    /* 'p_rtn_logif' is allowed to be NULL */
-    
-    
-    int rtn = FPP_ERR_FCI;
-    fpp_log_if_cmd_t cmd_to_fci = {0};
-    
-    /* prepare data */
