+    
+    
+    /* configure Egress QoS schedulers for emac0 */
+    /* ========================================= */
+    if (FPP_ERR_OK == rtn)
+    {
+        fpp_qos_scheduler_cmd_t sch = {0};
+        
+        /* scheduler 0 */
+        /* ----------- */
+        if (FPP_ERR_OK == rtn)
+        {
+            /* get data from PFE and store them in the local variable "sch" */
+            rtn = demo_qos_sch_get_by_id(p_cl, &sch, "emac0", 0u);
