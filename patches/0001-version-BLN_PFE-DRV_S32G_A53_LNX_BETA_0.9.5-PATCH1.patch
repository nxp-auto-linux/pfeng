-    assert(NULL != p_cl);
-    assert(NULL != p_rtn_logif);
-    
-    
-    int rtn = FPP_ERR_FCI;
-    
-    fpp_log_if_cmd_t cmd_to_fci = {0};
-    fpp_log_if_cmd_t reply_from_fci = {0};
