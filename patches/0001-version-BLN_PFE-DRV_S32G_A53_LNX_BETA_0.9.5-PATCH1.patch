-    ntoh_logif(&reply_from_fci);  /* set correct byte order */
-    
-    /* query loop */
-    while ((FPP_ERR_OK == rtn) && (id != (reply_from_fci.id)))
-    {
-        cmd_to_fci.action = FPP_ACTION_QUERY_CONT;
