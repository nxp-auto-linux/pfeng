-    assert(NULL != p_phyif_name);
-    
-    
-    int rtn = FPP_ERR_FCI;
-    
-    fpp_qos_queue_cmd_t cmd_to_fci = {0};
-    fpp_qos_queue_cmd_t reply_from_fci = {0};
-    unsigned short reply_length = 0u;
-    
-    /* prepare data */
-    rtn = set_text((cmd_to_fci.if_name), p_phyif_name, IFNAMSIZ);
-    
-    /* do the query */
-    if (FPP_ERR_OK == rtn)
-    {
-        /* query loop */
