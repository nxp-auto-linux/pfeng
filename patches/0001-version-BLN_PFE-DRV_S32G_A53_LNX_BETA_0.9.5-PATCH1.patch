-                            sizeof(fpp_qos_queue_cmd_t), (unsigned short*)(&cmd_to_fci),
-                            &reply_length, (unsigned short*)(&reply_from_fci));
-            ntoh_que(&reply_from_fci);  /* set correct byte order */
