+        }
+        
+        /* queue 5 (disabled) */
+        /* ------------------ */
+        if (FPP_ERR_OK == rtn)
+        {
+            /* get data from PFE and store them in the local variable "que" */
+            rtn = demo_qos_que_get_by_id(p_cl, &que, "emac0", 5u);
+            if (FPP_ERR_OK == rtn)
