+        cmd_to_fci.action = FPP_ACTION_QUERY_CONT;
+        rtn = fci_query(p_cl, FPP_CMD_IP_ROUTE,
+                        sizeof(fpp_rt_cmd_t), (unsigned short*)(&cmd_to_fci),
