-                                    const fpp_ct_cmd_t* p_ct_data)
-{
-    assert(NULL != p_cl);
-    assert(NULL != p_rtn_ct);
-    
-    
-    int rtn = FPP_ERR_FCI;
-    
-    fpp_ct_cmd_t cmd_to_fci = {0};
